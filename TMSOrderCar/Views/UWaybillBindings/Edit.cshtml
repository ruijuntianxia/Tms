@model TMSOrderCar.Models.UWaybillBinding

@{
    ViewData["Title"] = "修改";
}

<h2>修改</h2>

<h4>订单绑定车牌</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="DocEntry" />
            <div class="form-group">
                <label asp-for="OrderNo" class="control-label"></label>
                <input asp-for="OrderNo" class="form-control" />
                <span asp-validation-for="OrderNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Carnumber" class="control-label"></label>
                <input asp-for="Carnumber" class="form-control" />
                <span asp-validation-for="Carnumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ExpectDateLoad" class="control-label"></label>
                <input asp-for="ExpectDateLoad" class="form-control" />
                <span asp-validation-for="ExpectDateLoad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ActualDateLoad" class="control-label"></label>
                <input asp-for="ActualDateLoad" class="form-control" />
                <span asp-validation-for="ActualDateLoad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CreateUser" class="control-label"></label>
                <input asp-for="CreateUser" class="form-control" readonly="readonly" />
                <span asp-validation-for="CreateUser" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CreateDate" class="control-label"></label>
                <input asp-for="CreateDate" class="form-control" readonly="readonly" />
                <span asp-validation-for="CreateDate" class="text-danger"></span>
            </div>


            <div class="form-group">
                <input type="submit" value="保存" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">返回</a>
</div>
<script>

    (function () {
        verifyToken();
    }());

</script>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
